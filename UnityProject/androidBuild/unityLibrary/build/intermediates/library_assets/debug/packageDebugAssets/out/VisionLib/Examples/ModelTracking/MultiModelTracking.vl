{
    "$schema": "https://docs.visionlib.com/v20.3.1/vl.schema.json",
    "type": "VisionLibTrackerConfig",
    "version": 1,
    "tracker": {
        "type": "multiModelTracker",
        "version": 1,
        "parameters": {
            "minInlierRatioInit": 0.75,
            "minInlierRatioTracking": 0.65,
            "laplaceThreshold": 98.2,
            "normalThreshold": 0.3,
            "lineGradientThreshold": 40,
            "lineSearchLengthInitRelative": 0.05125,
            "lineSearchLengthTrackingRelative": 0.03125,
            "keyFrameDistance": 50,
            "metric": "m",
            "showLineModel": true,
            "showLineModelLost": false,
            "autoInit": true,
            "lineModelBufferSize": 1000,
            "learnTemplates": false,
            "anchors": [
                {
                    "name": "VLCar",
                    "models": [
                        {
                            "uri": "project_dir:/VLMiniCar.obj"
                        }
                    ]
                },
                {
                    "name": "VLSportsCar",
                    "models": [
                        {
                            "uri": "project_dir:/VLSportsCar.obj"
                        }
                    ]
                },
                {
                    "name": "VLCaravan",
                    "models": [
                        {
                            "uri": "project_dir:/VLCaravan.obj"
                        }
                    ]
                }
            ]
        }
    }
}