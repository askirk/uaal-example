{
    "$schema": "https://docs.visionlib.com/v20.3.1/vl.schema.json",
    "type": "VisionLibTrackerConfig",
    "version": 1,
    "meta": {
        "name": "Simple Visionlib Car Tracker for HoloLens",
        "description": "Simple model tracking configuration file",
        "author": "VisionLib"
    },
    "tracker": {
        "type": "hololensModelTracker",
        "version": 1,
        "parameters": {
            "modelURI": "project_dir:VLMiniCar.obj",
            "useColor": false,
            "metric": "m",
            "initPose": {
                "type": "visionlib",
                "t": [
                    -0.0012428760528564454,
                    0.008103832602500916,
                    0.7171652317047119
                ],
                "q": [
                    -0.2433263510465622,
                    -0.387615442276001,
                    0.8840279579162598,
                    0.09508475661277771
                ]
            },
            "keyFrameDistance": 50,
            "laplaceThreshold": 1,
            "normalThreshold": 0.3,
            "minInlierRatioInit": 0.7,
            "minInlierRatioTracking": 0.6,
            "lineSearchLengthTrackingRelative": 0.04,
            "maxFramesFeaturePrediction": 3,
            "showLineModel": true,
            "showLineModelCriticalColor": [
                255,
                0,
                127
            ],
            "extendibleTracking": true
        }
    },
    "input": {
        "useImageSource": "cameraHololens",
        "imageSources": [
            {
                "name": "cameraHololens",
                "type": "camera",
                "data": {
                    "undistort": true,
                    "scale": 0.5,
                    "cameramode": 0,
                    "framerate": 30
                }
            }
        ]
    }
}